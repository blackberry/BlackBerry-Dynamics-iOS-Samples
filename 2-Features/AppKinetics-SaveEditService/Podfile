
target 'AppKinetics-SaveEditService' do
  use_frameworks!
  platform :ios, '18.0'

  pod 'BlackBerryDynamics'

  target 'AppKinetics-SaveEditServiceUITests' do

    pod 'BlackBerryDynamicsAutomatedTestSupportLibrary'
  end

end

post_install do |installer|
  # Append configuration from default.xcconfig to configuration generated by Cocoapods
  if Dir.glob("**/default.xcconfig").first()
    system("find Pods -name 'Pods-*.xcconfig' -exec sh -c 'cat \"`find . -name default.xcconfig`\" >> $1' -- {} \\;")
  end
end
